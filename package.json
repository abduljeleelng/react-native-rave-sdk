{
  "name": "react-native-rave-sdk",
  "version": "1.0.0",
  "description": "React Native Fluterwave Rave SDK ",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "clean": "rimraf ./lib",
    "build": "npx tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepublishOnly": "npm test && npm run lint && npm run clean",
    "prepare": "npm run build",
    "preversion": "npm run lint",
    "version": "",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abduljeleelng/react-native-rave-sdk.git"
  },
  "keywords": [
    "React-native",
    "flutterwave",
    "Rave",
    "Payment",
    "Pay",
    "SDK",
    "React",
    "Native",
    "API"
  ],
  "author": "Abduljeleel Yusuff <abduljeleelng@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/abduljeleelng/react-native-rave-sdk/issues"
  },
  "homepage": "https://github.com/abduljeleelng/react-native-rave-sdk#readme",
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "all-contributors-cli": "^6.24.0",
    "jest": "^29.3.1",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-webview": "*"
  },
  "dependencies": {
    "react-native-autoheight-webview": "^1.6.5"
  }
}
